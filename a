#include  "bits/stdc++.h"
using namespace std;
#define         int                 long long
#define         pb                  push_back
#define         si                  set<int>
#define         vi                  vector<int>
#define         mod                 1000000007
#define         sz(x)               (int)((x).size())
#define         all(x)              (x).begin(),(x).end()
#define         FOR(i,a,b)          for(int i=a;i<b;i++)
#define         INF                 1000000000000000000
#define         endl                "\n"

template<typename T,typename T1>T amax(T &a,T1 b){if(b>a)a=b;return a;}
template<typename T,typename T1>T amin(T &a,T1 b){if(b<a)a=b;return a;}

int annu( vi v1, vi v2 , vi v3,int r)
{
	vi ans;
	FOR(i,0,r){
		int a= lower_bound(all(v2),v1[i])-v2.begin();
		if(a==sz(v2)){
			a--;
		}
		int b= lower_bound(all(v3),v1[i])-v3.begin();
		if(b==sz(v3)){
			b--;
		}
		vi p1,p2;
		p1.pb(a);
		p2.pb(b);
		if(sz(v2)>1 && a>0){
			p1.pb(a-1);
		}
		if(sz(v3)>1 && b>0){
			p2.pb(b-1);
		}
		FOR(j,0,sz(p1)){
			int c= lower_bound(all(v3),p1[j])-v3.begin();
			if(c==sz(v3)){
				c--;
			}
			p2.pb(c);
			if(sz(v3)>1 && c>0){
				p2.pb(c-1);
			}
		}
		FOR(j,0,sz(p1)){
			FOR(k,0,sz(p2)){
				int a= pow(abs(v1[i]-v2[p1[j]]),2)+ pow(abs(v1[i]-v3[p2[k]]),2)+ pow( abs( v2[p1[j]]-v3[p2[k]]),2);
				ans.pb(a);
			}
		}
	}
	sort(all(ans));
	return ans[0];
}
void SOLVE()
{
	int r,g,b;
	cin>>r>>g>>b;
	vi v1(r),v2(g),v3(b);
	FOR(i,0,r){
		cin>>v1[i];
	}
	FOR(i,0,g){
		cin>>v2[i];
	}
	FOR(i,0,b){
		cin>>v3[i];
	}
	sort(all(v1));
	sort(all(v2));
	sort(all(v3));
	vi fnl;
	int a= annu(v1,v2,v3,r);
	fnl.pb(a);
	int l= annu(v2,v1,v3,g);
	fnl.pb(l);
	int c= annu(v3,v1,v2,b);
	fnl.pb(c);
	sort(all(fnl));
	cout<<fnl[0]<<endl;
	
}
signed main()
{
	ios_base::sync_with_stdio(false);cin.tie(0);cout.tie(0);
	int TC =1;
	//cin>>TC;
	while(TC--){SOLVE();}
	return 0;
}
