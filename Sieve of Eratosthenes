int primes[1000001];
   
        int maxN=1000000;
        for(int i=1;i<=maxN;i++)
        {
            primes[i]=1;
        }
        primes[0]=primes[1]=0;
        for( long long int i=2;i*i<= maxN;i++)
        {
            if(primes[i])
            {
                for(long long int j=i*i;j<=maxN;j=j+i)
                {
                    primes[j]=0;
                }
            }
       }
