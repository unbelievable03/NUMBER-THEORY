
        int pr[1000001];
        int maxN=1000000;
        FOR(i,1,maxN+1){
            primes[i]=1;
        }
        pr[0]=pr[1]=0;
        FOR(i,2,sqrt(maxN)+1){
            if(pr[i]){
            for(int j=i*i;j<=maxN;j+=i){
                    pr[j]=0;
                }
            }
       }
