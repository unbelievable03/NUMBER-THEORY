#include<bits/stdc++.h>
using namespace std;
int binaryexpo(int a , int n)
{
    int res=1;
    while(n)
    {
       if(n%2==0)
       {
           a= a*a;
           n=n/2;
       }
        else if (n%2!=0)
        {
            res= res*a;
            n--;
        }
    }
    return res;
}
int main()
{
    int a,n;
    cin>>a>>n;
    int answer = binaryexpo(a,n);
    cout<<answer<<endl;
}
