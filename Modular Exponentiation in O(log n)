#include<bits/stdc++.h>
using namespace std;
int modularexpo(int a , int n, int p)
{
    int res=1;
    while(n)
    {
       if(n%2==0)
       {
           a= (a*a)%p;
           n=n/2;
       }
        else if (n%2!=0)
        {
            res= (res*a)%p;
            n--;
        }
    }
    return res;
}
int main()
{
    int a,n,p;
    cin>>a>>n>>p;
    int answer = modularexpo(a,n,p);
    cout<<answer<<endl;
}
